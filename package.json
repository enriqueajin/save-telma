{
  "name": "save-telma",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tailwind-dev": "npx tailwindcss -i ./src/index.css -o ./styles.css --watch",
    "tailwind-prod": "npx tailwindcss -i ./src/index.css -o ./styles.css",
